<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="transmove_sale_documents_view_tree" model="ir.ui.view">
        <field name="name">transmove_sale_documents.view.tree</field>
        <field name="model">transmove.documents</field>
        <field name="arch" type="xml">
            <tree string="">
                <field name="name" />
                <field name="file" widget="binary" />
            </tree>
        </field>
    </record>
    <record id="transmove_documents_view_form" model="ir.ui.view">
        <field name="name">transmove.documents.view.form</field>
        <field name="model">transmove.documents</field>
        <field name="arch" type="xml">
            <form string="">
                <sheet>
                    <group>
                        <field name="name" />
                        <field name="file" />
                    </group>
                </sheet>
            </form>
        </field>
    </record>
</odoo>
